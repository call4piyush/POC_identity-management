{
  "realm": "demo-realm",
  "enabled": true,
  "clients": [
    {
      "clientId": "springboot-api",
      "name": "Spring Boot API",
      "publicClient": true,
      "directAccessGrantsEnabled": true,
      "redirectUris": ["*"]
    }
  ],
  "users": [
    {
      "username": "api-user",
      "enabled": true,
      "credentials": [
        { "type": "password", "value": "password", "temporary": false }
      ],
      "realmRoles": ["user"]
    }
  ],
  "roles": {
    "realm": [
      { "name": "user" },
      { "name": "admin" }
    ]
  }
}


